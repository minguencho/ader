#define _CRT_SECURE_NO_WARNINGS
#include <iostream>

int main(void)
{
	int arr[] = { 1,10,4,9,2,3,8,5,7,6 }; //10개의 정수를 가진 배열 설정.
	printf("숫자를 입력하세요.\n");
	int y;
	scanf("%d", &y); // 기준이 되는 y 입력 받음.
		for (int x = 0; x < 10; x++) { //for문과 if문을 통해서 y값보다 작은 정수를 찾음.
			if (y >arr[x]) {
				printf("%d ", arr[x]);
			}
		else{}
		
	}
	
	return 0;
}
