#define _CRT_SECURE_NO_WARNINGS
#include <iostream>

int main(void)
{
	int x,y;
	int a,b,c,;
	scanf("%d", &x); //두 수를 입력받음.
	scanf("%d", &y);
	a =(y % 10)*x;
	b =((y % 100) / 10) * x;
	c = (y / 100) * x;
	
	printf("  %d\n", );		//x와 y의 1의 자리수를 곱함
	printf(" %d\n", b); 		//x와 y의 10의 자리수를 곱함
	printf("%d\n", c);		//x와 y의 100의 자리수를 곱함
	printf("%d",a+b*10+c*100);	//x와 y의 곱한 값.
	return 0;
}
